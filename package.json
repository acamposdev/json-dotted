{
    "name": "json-dotted",
    "version": "1.0.0",
    "description": "Utility for retrieve absolute path of property object",
    "main": "./lib/app.js",
    "bin": {
        "doterize": "./bin/json-dotted.js"
    },
    "scripts": {
        "test": "mocha -R spec ./test/*.js",
        "coverage": "istanbul cover _mocha --dir './docs/coverage' 'test/*.js' -- -R spec",
        "gendoc": "jsdoc './app.js' -c './node_modules/ink-docstrap/template/jsdoc.conf.json' -d './docs/api docs/' -t './node_modules/ink-docstrap/template'"
    },
    "author": "acamposdev",
    "license": "MIT",
    "devDependencies": {
        "ink-docstrap": "^1.3.2",
        "mochawesome": "^3.0.0"
    },
    "directories": {
        "lib": "lib",
        "test": "test"
    },
    "dependencies": {}
}
